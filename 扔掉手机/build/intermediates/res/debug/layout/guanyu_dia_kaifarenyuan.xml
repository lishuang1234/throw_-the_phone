<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="300dp"
    android:background="@drawable/setting_shape_rectangle" >

    <RelativeLayout
        android:id="@+id/guanyu_dia_ll_line"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/activities_title_back"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/guanyu_dia_banbenggengxin_tx_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="开发人员"
            android:textColor="@android:color/white"
            android:textSize="20sp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/guanyu_dia_ll_line"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="程序开发：李双"
        android:textColor="@color/green_light"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="UI设计：谭泽斌"
        android:textColor="@color/green_light"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="软件测试：王威"
        android:textColor="@color/green_light"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="文档整理：谭欣悦"
        android:textColor="@color/green_light"
        android:textSize="15sp" />

    <Button
        android:id="@+id/guanyu_dia_btn_kaifarenyuan_entrue"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/setting_shape_rectangle_press"
        android:text="确认"
        android:textColor="@color/settingAc_tx_content" />

</RelativeLayout><!-- From: file:/home/ls/androidDevelop/androidStudioWorkSpace/ls/throw_-the_phone/throw_-the_phone/扔掉手机/res/layout/guanyu_dia_kaifarenyuan.xml -->